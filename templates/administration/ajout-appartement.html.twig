{% extends 'base.html.twig' %}

{% block title %}Liste Locataires{% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: red; padding: 2px 6px; }
</style>

<div class="example-wrapper">
    <h1> ajout appartements</h1>

    
   
 

  <form method="post">
    {{ form_row(form.adresse) }}
    {{ form_row(form.complement) }}
    {{ form_row(form.ville) }}
    {{ form_row(form.code_postal) }}
    {{ form_row(form.loyer) }}
    {{ form_row(form.charges) }}    
    {{ form_rest(form) }}
    <button type="submit">Ajouter</button>
</form>
    

</div>
{% endblock %}
